{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/go/**",
        "dynamicLinks": true
      },
      {
        "source": "/game/**",
        "dynamicLinks": true
      },
      {
        "source": "!/go/**",
        "destination": "/index.html"
      },
      {
        "source": "!/game/**",
        "destination": "/index.html"
      }
    ]
  }
}
